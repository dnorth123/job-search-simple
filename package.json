{
  "name": "job-search-mvp",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:report": "playwright show-report",
    "test:cypress": "cypress run",
    "test:cypress:open": "cypress open",
    "test:cypress:headed": "cypress run --headed",
    "test:cypress:staging": "cypress run --env environment=staging",
    "test:cypress:production": "cypress run --env environment=production",
    "sync-beta": "node scripts/sync-beta-list.js",
    "test:sync": "node scripts/test-sync.js",
    "test:email": "node scripts/test-email.js",
    "email-server": "node server/email-api.js",
    "dev:email": "concurrently \"npm run dev\" \"npm run email-server\"",
    "install-server": "node scripts/install-server-deps.js",
    "setup-gmail-filters": "node scripts/setup-gmail-filters.js",
    "dev:start": "./scripts/dev-server.sh start",
    "dev:stop": "./scripts/dev-server.sh stop",
    "dev:status": "./scripts/dev-server.sh status",
    "dev:clean": "./scripts/dev-server.sh clean"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.52.1",
    "cors": "^2.8.5",
    "dotenv": "^17.2.1",
    "express": "^5.1.0",
    "lucide-react": "^0.541.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "tailwindcss": "^3.4.17"
  },
  "serverDependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.30.1",
    "@playwright/test": "^1.54.1",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.16",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.3.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/jest": "^29.5.12",
    "@types/node": "^24.1.0",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react": "^4.6.0",
    "autoprefixer": "^10.4.21",
    "concurrently": "^8.2.2",
    "cypress": "^14.5.3",
    "eslint": "^9.30.1",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "playwright": "^1.54.1",
    "postcss": "^8.5.6",
    "terser": "^5.43.1",
    "ts-jest": "^29.1.2",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.35.1",
    "vite": "^7.0.4"
  }
}
